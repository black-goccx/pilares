                        <<INTENTO DE JAVA>>
function idcomp () {
    let id = parseStringe (document.getElementById("name").value.trim());
    let pass = parseStringe (document.getElementById("pass").value.trim());

    if (id === "Joseph" && pass === "1521") {
        document.getElementById("name").textContent = "Correcto";
        document.getElementById("name").style.color = "red";

        window.location.href = "Calculadora.html";

    }else {
        
            alert("Nombre o contraseña incorrectos");
        }
    }


