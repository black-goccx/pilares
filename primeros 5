let numero;
while (true) {
numero = parseFloat(prompt("Ingrese un número impar: "));
if (isNaN (numero) ){ 
    alert ("Debes ingresar un número");
 }else if ( numero % 2 === 0 ) {
    alert("Tu número debe ser impar: " )
    
 }else {("Tu numero impar es: " + numero)

break;
}   
}

