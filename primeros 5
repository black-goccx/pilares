===============version 1==============
let cantidad = parseFloat(prompt("¡Cuantos número vas a ingresar?: "));
if (isNaN (cantidad) || cantidad <= 0) {
    alert ("Ingresa un valor válido");

}else {
    let suma = 0;
    
for (let i = 1; i<= cantidad; i++) {
     let numero = parseFloat(prompt(`introduce el numero ${i}:` ));

if (isNaN(numero)) {
    alert ("Ingresa un valor válido");
    i--;
}else {
    suma += numero;
} 
    
}
    let media = suma / cantidad;
    alert (`La media es: ${media}`);
}
===================version 2==================
No termina el programa cada que hay un error

while (true) {
    let cantidad = parseFloat(prompt("¿Cuantos número vas a ingresar?: "));
    
    if (isNaN (cantidad) || cantidad <= 0){
    alert ("Ingresa un valor válido");

}else {
    let suma = 0;
    
for (let i = 1; i<= cantidad; i++) {
     let numero = parseFloat(prompt(`introduce el numero ${i}:` ));

if (isNaN(numero)) {
    alert ("Ingresa un valor válido");
    i--;
}else {
    suma += numero;
} 
    
}
    let media = suma / cantidad;
    alert (`La media es: ${media}`);

}
}

