    function logica() {
        let n1 = parseFloat(document.getElementById("num1").value);
        let n2 = parseFloat(document.getElementById("num2").value);

let resultado1 = n1 + " y " + n2;

/*if (n1 < 0 || n1 === 15)  {
      let resultado2 = "Número negativo o igual a 15"
  } else {
    let resultado2 = numero - 15;
  } 
   
         
        if (isNaN(n1) || isNaN(n2)) {
            document.getElementById("total").value = "INGRESA VALORES NUMÉRICOS";
            return;
        } */
   
   
    
    
        document.getElementById("total1").value = resultado1;
        //document.getElementById("total2").value = resultado2;
    } 
 
</script>

